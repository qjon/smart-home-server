    "karma-jasmine-html-reporter": "^1.4.0",
    "moment": "^2.25.3",
    "ng-packagr": "^10.1.1",
    "protractor": "~5.4.0",
    "replace-in-file": "^6.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
