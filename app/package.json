{
  "name": "smart-home-app",
  "version": "3.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=smart-home --proxy-config proxy.config.json",
    "start:ri-home-assistant": "ng serve --project=ri-home-assistant --proxy-config proxy.config.json",
    "build:core": "ng build --project=core",
    "build:ri-home-assistant": "ng build --prod --output-hashing=none --project=ri-home-assistant && npm run package:ha",
    "build:weather-stations": "ng build --project=weather-stations",
    "build": "node scripts/timestamp.js && ng build",
    "lint": "ng lint",
    "package:ha": "cat ../dist/ri-home-assistant/runtime-es2015.js ../dist/ri-home-assistant/polyfills-es2015.js ../dist/ri-home-assistant/main-es2015.js > ../dist/ri-home-assistant/home-assistant-element.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/elements": "^8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngrx/effects": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "@ngrx/store-devtools": "^7.3.0",
    "@types/lodash": "^4.14.150",
    "angular-highcharts": "^8.0.3",
    "angular2-notifications": "^9.0.0",
    "document-register-element": "^1.7.2",
    "highcharts": "^8.0.0",
    "lodash": "^4.17.15",
    "material-icons": "^0.3.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.22",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.17.21",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
